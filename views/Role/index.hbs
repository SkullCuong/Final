<div class='row'>
  <div class='col-2'>
    <div
      class='card'
      style='border-radius: 30px; width:150px; margin-bottom: 20px;'
    >
      <a class='Add_btn' href=''><i class='fa-solid fa-add'></i> Add More</a>
    </div>
  </div>
</div>

